{
  "name": "downtimebot",
  "version": "0.0.1",
  "description": "Discord Downtime Tracker",
  "scripts": {
    "prebuild": "rimraf out",
    "build": "tsc -p tsconfig.json",
    "start": "node out/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-pattern '/node_modules/' ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx --ignore-pattern '/node_modules/' ./src"
  },
  "main": "dist/index.js",
  "repository": "https://github.com/mawburn/downtimebot",
  "author": "mawburn",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "^17.0.24",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0"
  }
}
